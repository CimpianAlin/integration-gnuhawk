<!DOCTYPE properties PUBLIC "-//JTRS//DTD SCA V2.2.2 PRF//EN" "properties.dtd">
<properties>
  <simple id="samples_per_symbol" mode="readwrite" type="float">
    <description>(float) Number of samples per symbol of signal</description>
    <value>4.0</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="rolloff" mode="readwrite" type="float">
    <description>(float) Rolloff factor of signal</description>
    <value>0.35</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="filter_size" mode="readwrite" type="long">
    <description>(int)   Size (in taps) of the filter</description>
    <value>45</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="bandwidth" mode="readwrite" type="float">
    <description> (float) Loop bandwidth</description>
    <value>0.0628318</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
</properties>
