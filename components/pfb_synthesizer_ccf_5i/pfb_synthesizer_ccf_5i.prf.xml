<!DOCTYPE properties PUBLIC "-//JTRS//DTD SCA V2.2.2 PRF//EN" "properties.dtd">
<properties>
  <simple id="numchans" mode="readonly" type="ulong">
    <description>unsigned integer) Specifies the number of channels</description>
    <value>5</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simplesequence id="taps" mode="readwrite" type="float">
    <description>(vector/list of floats) The prototype filter to populate the filterbank.</description>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simplesequence>
  <simple id="twox" mode="readwrite" type="boolean">
    <description>(bool) use 2x oversampling or not (default is no)</description>
    <value>False</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simplesequence id="channel_map" mode="readwrite" type="float">
    <kind kindtype="configure"/>
    <action type="external"/>
  </simplesequence>
</properties>
