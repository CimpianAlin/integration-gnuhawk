<!DOCTYPE properties PUBLIC "-//JTRS//DTD SCA V2.2.2 PRF//EN" "properties.dtd">
<properties>
  <simple id="itemsize" mode="readonly" name="itemsize" type="long">
    <value>2</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="filename" mode="readwrite" name="filename" type="string">
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="repeat" mode="readwrite" name="repeat" type="boolean">
    <value>false</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="seek_point" mode="readwrite" name="seek_point" type="long">
    <value>1</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="whence" mode="readwrite" name="whence" type="long">
    <value>1</value>
    <enumerations>
      <enumeration label="SEEK_SET" value="0"/>
      <enumeration label="SEEK_CUR" value="1"/>
      <enumeration label="SEEK_END" value="2"/>
    </enumerations>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="seek" mode="readonly" name="seek" type="boolean">
    <value>false</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
</properties>
