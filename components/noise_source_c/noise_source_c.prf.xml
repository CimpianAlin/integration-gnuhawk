  <simple id="seed" mode="readwrite" type="long">
    <value>0</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="transfer_size" mode="readwrite" type="long">
    <value>8192</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
</properties>
