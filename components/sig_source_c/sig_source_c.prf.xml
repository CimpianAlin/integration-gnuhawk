<!DOCTYPE properties PUBLIC "-//JTRS//DTD SCA V2.2.2 PRF//EN" "properties.dtd">
<properties>
  <simple id="sampling_freq" mode="readwrite" type="double">
    <value>10000.0</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="waveform" mode="readwrite" type="long">
    <value>101</value>
    <enumerations>
      <enumeration label="CONST_WAVE" value="100"/>
      <enumeration label="SIN_WAVE" value="101"/>
      <enumeration label="COS_WAVE" value="102"/>
      <enumeration label="SQR_WAVE" value="103"/>
      <enumeration label="TRI_WAVE" value="104"/>
      <enumeration label="SAW_WAVE" value="105"/>
    </enumerations>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="frequency" mode="readwrite" type="double">
    <value>10.0</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="amplitude" mode="readwrite" type="double">
    <value>100.0</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="offset" mode="readwrite" type="float" complex="true">
    <value>0+j0</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="transfer_size" mode="readwrite" type="long">
    <value>8192</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
</properties>
