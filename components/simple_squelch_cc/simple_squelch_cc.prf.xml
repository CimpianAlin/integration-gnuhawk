<!DOCTYPE properties PUBLIC "-//JTRS//DTD SCA V2.2.2 PRF//EN" "properties.dtd">
<properties>
  <simple id="threshold_db" mode="readwrite" name="threshold_db" type="double">
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="alpha" mode="readwrite" name="alpha" type="double">
    <value>0.0001</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simplesequence id="squelch_range" mode="readwrite" name="squelch_range" type="float">
    <kind kindtype="configure"/>
    <action type="external"/>
  </simplesequence>
  <simple id="unmuted" mode="readwrite" name="unmuted" type="boolean">
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
</properties>
