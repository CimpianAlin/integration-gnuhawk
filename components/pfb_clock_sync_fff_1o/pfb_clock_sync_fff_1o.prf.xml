<!DOCTYPE properties PUBLIC "-//JTRS//DTD SCA V2.2.2 PRF//EN" "properties.dtd">
<properties>
  <simple id="sps" mode="readwrite" name="sps" type="double">
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="gain" mode="readwrite" name="gain" type="float">
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simplesequence id="taps" mode="readwrite" name="taps" type="float">
    <kind kindtype="configure"/>
    <action type="external"/>
  </simplesequence>
  <simple id="filter_size" mode="readwrite" name="filter_size" type="ulong">
    <value>32</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="init_phase" mode="readwrite" name="init_phase" type="float">
    <value>0</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="max_rate_deviation" mode="readwrite" name="max_rate_deviation" type="float">
    <value>1.5</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="osps" mode="readwrite" name="osps" type="long">
    <value>1</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="damping_factor" mode="readwrite" name="damping_factor" type="float">
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="alpha" mode="readwrite" name="alpha" type="float">
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="beta" mode="readwrite" name="beta" type="float">
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="clock_rate" mode="readwrite" name="clock_rate" type="float">
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
</properties>
